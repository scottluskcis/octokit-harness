{
  "name": "octokit-harness",
  "version": "1.0.0",
  "description": "A flexible toolkit for working with GitHub API using Octokit",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "octokit-harness": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
