    "undici": "^7.4.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.2",
